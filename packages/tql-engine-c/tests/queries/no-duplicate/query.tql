#language 'typescript'

-- Ensure that we don't repeatedly yield the query if multiple clauses match.
fn main() {
  > class_declaration {
    @decorator_name <- *.decorator > call_expression.function;
    @class_name <- .name;

    (
      text_eq(@decorator_name, 'Controller') ||
      text_eq(@class_name, 'Class1') ||
      text_eq(@decorator_name, 'Controller') ||
      text_eq(@class_name, 'Class1')
    );
  }
}
