{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tql",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "query"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "--"
          },
          {
            "type": "PATTERN",
            "value": "[^\\r\\n\\u2028\\u2029]*"
          }
        ]
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z_]+"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_]"
            }
          }
        ]
      }
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "SYMBOL",
                    "name": "unescaped_single_string_fragment"
                  },
                  "named": true,
                  "value": "string_fragment"
                },
                {
                  "type": "SYMBOL",
                  "name": "escape_sequence"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "unescaped_single_string_fragment": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^'\\\\\\r\\n]+"
        }
      }
    },
    "escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^xu0-7]"
              },
              {
                "type": "PATTERN",
                "value": "[0-7]{1,3}"
              },
              {
                "type": "PATTERN",
                "value": "x[0-9a-fA-F]{2}"
              },
              {
                "type": "PATTERN",
                "value": "u[0-9a-fA-F]{4}"
              },
              {
                "type": "PATTERN",
                "value": "u\\{[0-9a-fA-F]+\\}"
              },
              {
                "type": "PATTERN",
                "value": "[\\r?][\\n\\u2028\\u2029]"
              }
            ]
          }
        ]
      }
    },
    "query": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "selector",
          "content": {
            "type": "SYMBOL",
            "name": "_selector"
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "FIELD",
                "name": "statement",
                "content": {
                  "type": "SYMBOL",
                  "name": "_statement"
                }
              }
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        }
      ]
    },
    "variable_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "query"
        },
        {
          "type": "SYMBOL",
          "name": "assignment"
        },
        {
          "type": "SYMBOL",
          "name": "condition"
        }
      ]
    },
    "_pure_selector": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "universal_selector"
        },
        {
          "type": "SYMBOL",
          "name": "node_type_selector"
        },
        {
          "type": "SYMBOL",
          "name": "field_name_selector"
        },
        {
          "type": "SYMBOL",
          "name": "child_selector"
        },
        {
          "type": "SYMBOL",
          "name": "descendant_selector"
        },
        {
          "type": "SYMBOL",
          "name": "variable_identifier"
        }
      ]
    },
    "_selector": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "pure_selector",
            "content": {
              "type": "SYMBOL",
              "name": "_pure_selector"
            }
          },
          {
            "type": "FIELD",
            "name": "node_operation",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "["
                    },
                    {
                      "type": "REPEAT",
                      "content": {
                        "type": "SYMBOL",
                        "name": "inline_node_operation"
                      }
                    },
                    {
                      "type": "STRING",
                      "value": "]"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          }
        ]
      }
    },
    "universal_selector": {
      "type": "STRING",
      "value": "*"
    },
    "node_type_selector": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "node_type"
    },
    "field_name_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "parent_field",
                "content": {
                  "type": "SYMBOL",
                  "name": "_selector"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "."
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            },
            "named": true,
            "value": "field_name"
          }
        ]
      }
    },
    "child_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "parent",
                "content": {
                  "type": "SYMBOL",
                  "name": "_selector"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": ">"
          },
          {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          }
        ]
      }
    },
    "descendant_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "parent",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_descendant_operator"
          },
          {
            "type": "FIELD",
            "name": "descendant",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          }
        ]
      }
    },
    "inline_node_operation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline_node_assignment"
        }
      ]
    },
    "inline_node_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_identifier"
        },
        {
          "type": "STRING",
          "value": "<-"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "_expression": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_selector"
          }
        ]
      }
    },
    "condition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_parenthesized_condition"
        },
        {
          "type": "SYMBOL",
          "name": "empty_condition"
        },
        {
          "type": "SYMBOL",
          "name": "text_eq_condition"
        },
        {
          "type": "SYMBOL",
          "name": "or_condition"
        },
        {
          "type": "SYMBOL",
          "name": "and_condition"
        }
      ]
    },
    "_parenthesized_condition": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "SYMBOL",
            "name": "condition"
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "empty_condition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "text_eq_condition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "or_condition": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "condition"
          },
          {
            "type": "STRING",
            "value": "||"
          },
          {
            "type": "SYMBOL",
            "name": "condition"
          }
        ]
      }
    },
    "and_condition": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "condition"
          },
          {
            "type": "STRING",
            "value": "&&"
          },
          {
            "type": "SYMBOL",
            "name": "condition"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_descendant_operator"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {
    "global": []
  }
}