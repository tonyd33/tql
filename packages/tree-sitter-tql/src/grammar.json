{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tql",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "directive"
          },
          {
            "type": "SYMBOL",
            "name": "function_definition"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "--"
          },
          {
            "type": "PATTERN",
            "value": "[^\\r\\n\\u2028\\u2029]*"
          }
        ]
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z_]+"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_]"
            }
          }
        ]
      }
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "SYMBOL",
                    "name": "unescaped_single_string_fragment"
                  },
                  "named": true,
                  "value": "string_fragment"
                },
                {
                  "type": "SYMBOL",
                  "name": "escape_sequence"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "unescaped_single_string_fragment": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^'\\\\\\r\\n]+"
        }
      }
    },
    "escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^xu0-7]"
              },
              {
                "type": "PATTERN",
                "value": "[0-7]{1,3}"
              },
              {
                "type": "PATTERN",
                "value": "x[0-9a-fA-F]{2}"
              },
              {
                "type": "PATTERN",
                "value": "u[0-9a-fA-F]{4}"
              },
              {
                "type": "PATTERN",
                "value": "u\\{[0-9a-fA-F]+\\}"
              },
              {
                "type": "PATTERN",
                "value": "[\\r?][\\n\\u2028\\u2029]"
              }
            ]
          }
        ]
      }
    },
    "variable_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "include_directive"
            },
            {
              "type": "SYMBOL",
              "name": "target_lang_directive"
            }
          ]
        }
      ]
    },
    "target_lang_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "language"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "include_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "import"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "function_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "fn"
        },
        {
          "type": "FIELD",
          "name": "identifier",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "parameters",
                      "content": {
                        "type": "SYMBOL",
                        "name": "variable_identifier"
                      }
                    },
                    {
                      "type": "REPEAT",
                      "content": {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "STRING",
                            "value": ","
                          },
                          {
                            "type": "FIELD",
                            "name": "parameters",
                            "content": {
                              "type": "SYMBOL",
                              "name": "variable_identifier"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "statement",
                          "content": {
                            "type": "SYMBOL",
                            "name": "_statement"
                          }
                        },
                        {
                          "type": "REPEAT",
                          "content": {
                            "type": "SEQ",
                            "members": [
                              {
                                "type": "STRING",
                                "value": ";"
                              },
                              {
                                "type": "FIELD",
                                "name": "statement",
                                "content": {
                                  "type": "SYMBOL",
                                  "name": "_statement"
                                }
                              }
                            ]
                          }
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ";"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        }
      ]
    },
    "function_invocation": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "identifier",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "parameters",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_expression"
                      }
                    },
                    {
                      "type": "REPEAT",
                      "content": {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "STRING",
                            "value": ","
                          },
                          {
                            "type": "FIELD",
                            "name": "parameters",
                            "content": {
                              "type": "SYMBOL",
                              "name": "_expression"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_selector"
          },
          "named": true,
          "value": "selector"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_assignment"
          },
          "named": true,
          "value": "assignment"
        }
      ]
    },
    "_selector": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_parenthesized_selector"
        },
        {
          "type": "SYMBOL",
          "name": "self_selector"
        },
        {
          "type": "SYMBOL",
          "name": "node_type_selector"
        },
        {
          "type": "SYMBOL",
          "name": "field_name_selector"
        },
        {
          "type": "SYMBOL",
          "name": "child_selector"
        },
        {
          "type": "SYMBOL",
          "name": "descendant_selector"
        },
        {
          "type": "SYMBOL",
          "name": "block_selector"
        },
        {
          "type": "SYMBOL",
          "name": "variable_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "function_invocation"
        },
        {
          "type": "SYMBOL",
          "name": "negate_selector"
        },
        {
          "type": "SYMBOL",
          "name": "and_selector"
        },
        {
          "type": "SYMBOL",
          "name": "or_selector"
        }
      ]
    },
    "selector": {
      "type": "SYMBOL",
      "name": "_selector"
    },
    "_parenthesized_selector": {
      "type": "PREC",
      "value": 100,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "SYMBOL",
            "name": "_selector"
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "self_selector": {
      "type": "STRING",
      "value": "*"
    },
    "node_type_selector": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "identifier"
      },
      "named": true,
      "value": "node_type"
    },
    "field_name_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "parent",
                "content": {
                  "type": "SYMBOL",
                  "name": "_selector"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "."
          },
          {
            "type": "FIELD",
            "name": "field",
            "content": {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "identifier"
              },
              "named": true,
              "value": "field_name"
            }
          }
        ]
      }
    },
    "child_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "parent",
                "content": {
                  "type": "SYMBOL",
                  "name": "_selector"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": ">"
          },
          {
            "type": "FIELD",
            "name": "child",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          }
        ]
      }
    },
    "descendant_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "parent",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_descendant_operator"
          },
          {
            "type": "FIELD",
            "name": "descendant",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          }
        ]
      }
    },
    "block_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "parent",
                "content": {
                  "type": "SYMBOL",
                  "name": "_selector"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "{"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "FIELD",
                            "name": "statement",
                            "content": {
                              "type": "SYMBOL",
                              "name": "_statement"
                            }
                          },
                          {
                            "type": "REPEAT",
                            "content": {
                              "type": "SEQ",
                              "members": [
                                {
                                  "type": "STRING",
                                  "value": ";"
                                },
                                {
                                  "type": "FIELD",
                                  "name": "statement",
                                  "content": {
                                    "type": "SYMBOL",
                                    "name": "_statement"
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ";"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "}"
              }
            ]
          }
        ]
      }
    },
    "negate_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "!"
          },
          {
            "type": "SYMBOL",
            "name": "_selector"
          }
        ]
      }
    },
    "and_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          },
          {
            "type": "STRING",
            "value": "&&"
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          }
        ]
      }
    },
    "or_selector": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          },
          {
            "type": "STRING",
            "value": "||"
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "_selector"
            }
          }
        ]
      }
    },
    "_assignment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "explicit_assignment"
        }
      ]
    },
    "assignment": {
      "type": "SYMBOL",
      "name": "assignment"
    },
    "explicit_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "identifier",
          "content": {
            "type": "SYMBOL",
            "name": "variable_identifier"
          }
        },
        {
          "type": "STRING",
          "value": "<-"
        },
        {
          "type": "FIELD",
          "name": "expression",
          "content": {
            "type": "SYMBOL",
            "name": "_expression"
          }
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "selector"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "expression": {
      "type": "SYMBOL",
      "name": "_expression"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
