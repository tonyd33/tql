{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tql",
      "camelcase": "Tql",
      "title": "Tql",
      "scope": "source.tql",
      "file-types": ["tql"],
      "injection-regex": "^tql$",
      "class-name": "TreeSitterTql"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tql grammar for tree-sitter",
    "authors": [
      {
        "name": "Tony Du"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tql"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
